{
  "name": "participium",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "npm run test:setup && jest --runInBand",
    "test:unit": "jest --testPathPattern=test/unit --coverage --ci --passWithNoTests",
    "test:integration": "npm run test:setup && jest --testPathPattern=test/integrated --coverage --ci --runInBand --passWithNoTests",
    "test:coverage": "npm run test:setup && jest --coverage --runInBand",
    "test:ci": "npm run test:setup && jest --coverage --runInBand --ci --passWithNoTests",
    "test:unit:watch": "jest --testPathPattern=test/unit --watch",
    "test:integration:watch": "npm run test:setup && jest --testPathPattern=test/integrated --watch --runInBand",
    "test:watch": "npm run test:setup && jest --watch --runInBand",
    "test:setup": "docker-compose up -d test_db && sleep 5 && cross-env DATABASE_URL=postgresql://postgres:postgres@localhost:5433/participium_test_db?schema=public npx prisma migrate deploy",
    "test:reset": "docker-compose down test_db && docker volume rm participium-team-4_pgdata_test || echo Volume already removed && npm run test:setup",
    "test:cleanup": "docker-compose down test_db",
    "test:ci:unit": "jest --testPathPatterns=\"test/unit\" --coverage --ci --passWithNoTests",
    "test:ci:integration": "jest --testPathPatterns=\"test/integrated\" --coverage --ci --runInBand --passWithNoTests",
    "test:ci:all": "jest --coverage --runInBand --ci --passWithNoTests",
    "participium": "node setup-db.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.18.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-table": "^8.21.3",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "jose": "^6.1.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.552.0",
    "next": "16.0.1",
    "next-auth": "^4.24.13",
    "prisma": "^6.18.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-easy-crop": "^5.5.3",
    "react-leaflet": "^5.0.0",
    "react-leaflet-markercluster": "^5.0.0-rc.0",
    "tailwind-merge": "^3.3.1",
    "ts-node": "^10.9.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "cross-env": "^10.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "jest": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.5",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
}
